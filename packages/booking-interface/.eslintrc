{
  "extends": ["@iteam/eslint-config-node"],
  "overrides": [
    {
      "files": ["**/*.js"],
      "rules": {
        "no-unused-vars": [
          "error",
          {
            "vars": "all",
            "args": "after-used",
            "ignoreRestSiblings": true,
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
          }
        ]
      }
    }
  ]
}
